<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="CheckDetailInfo" table="check_detail_info">
		<id column="checkDetailid" name="checkDetailID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="clientid" name="clientID" type="java.lang.Long"></property>
		<property column="projectid" name="projectID" type="java.lang.Long"></property>
		<property column="issueid" name="issueID" type="java.lang.Long"></property>
		<property column="inquisitionid" name="inquisitionID" type="java.lang.Long"></property>				
		<property column="what" name="what" type="java.lang.String"></property>
		<property column="enWhat" name="enWhat" type="java.lang.String"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>	
		<property column="type" name="type" type="java.lang.Integer"></property>									
	</class>
</hibernate-mapping>