<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="EmailDraftInfo" table="email_draft_info">
		<id column="draftid" name="draftID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="acid" name="acID" type="java.lang.Long"></property>
		<property column="addressid" name="addressID"  type="java.lang.Long"></property>
		<property column="employeeid" name="employeeID" type="java.lang.Long"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>				
		<property column="content" name="content" type="java.lang.String"></property>	
		<property column="topic" name="topic" type="java.lang.String"></property>				
		<property column="att" name="att" type="java.lang.String"></property>	
		<property column="sendto" name="sendTo" type="java.lang.String"></property>				
		<property column="sendtime" name="sendTime" type="java.sql.Timestamp"></property>				
	</class>
</hibernate-mapping>