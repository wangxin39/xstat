<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="InquisitionTemplateInfo" table="inquisition_template_info">
		<id column="templateid" name="templateID" type="java.lang.Long">
			<generator class="identity"/>
		</id>		
		<property column="status" name="status" type="java.lang.Integer"></property>
		<property column="filetype" name="fileType" type="java.lang.Integer"></property>
		<property column="description" name="description" type="java.lang.String"></property>	
		<property column="name" name="name" type="java.lang.String"></property>
		<property column="filename" name="fileName" type="java.lang.String"></property>
		<property column="savepath" name="savePath" type="java.lang.String"></property>			
	</class>
</hibernate-mapping>