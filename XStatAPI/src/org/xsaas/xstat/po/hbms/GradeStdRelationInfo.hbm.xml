<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="GradeStdRelationInfo" table="grade_std_relation_info">
		<id column="stdRelationID" name="stdRelationID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="gradeStdID" name="gradeStdID" type="java.lang.Long"></property>	
		<property column="selectWeightID" name="selectWeightID" type="java.lang.Long"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>	
	</class>
</hibernate-mapping>