<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="ReqResultInfo" table="req_result_info">
		<id column="resultid" name="resultID" type="java.lang.Long">
			<generator class="identity"/>
		</id>		
		<property column="accountid" name="accountID" type="java.lang.Long"></property>
		<property column="reqid" name="reqID" type="java.lang.Long"></property>
		<property column="employeeid" name="employeeID" type="java.lang.Long"></property>
		<property column="reqtime" name="reqTime" type="java.sql.Timestamp"></property>																												
		<property column="title" name="title" type="java.lang.String"></property>	
		<property column="content" name="content" type="java.lang.String"></property>	
		<property column="status" name="status" type="java.lang.Integer"></property>			
	</class>
</hibernate-mapping>