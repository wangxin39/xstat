<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="VideoInfo" table="video_info">
		<id column="videoid" name="videoID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
	    <property column="clientid" name="clientID" type="java.lang.Long"></property>
	    <property column="informantid" name="informantID" type="java.lang.Long"></property>
		<property column="name" name="name" type="java.lang.String"></property>
		<property column="url" name="url" type="java.lang.String"></property>		
		<property column="addtime" name="addTime" type="java.sql.Timestamp"></property>		
		<property column="status" name="status" type="java.lang.Integer"></property>	
		<property column="englishname" name="englishName" type="java.lang.String"></property>									
	</class>
</hibernate-mapping>