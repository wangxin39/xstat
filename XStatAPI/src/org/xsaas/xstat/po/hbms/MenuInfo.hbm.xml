<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="MenuInfo" table="menu_info">
		<id column="menuid" name="menuID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="menu" name="menu" type="java.lang.String"></property>
		<property column="type" name="type" type="java.lang.Integer"></property>				
		<property column="parentid" name="parentID" type="java.lang.Long"></property>	
		<property column="url" name="url" type="java.lang.String"></property>	
		<property column="status" name="status" type="java.lang.Integer"></property>									
	</class>
</hibernate-mapping>