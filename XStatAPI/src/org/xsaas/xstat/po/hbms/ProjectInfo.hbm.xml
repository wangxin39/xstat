<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="ProjectInfo" table="project_info">
		<id column="projectid" name="projectID" type="java.lang.Long">
			<generator class="identity"/>
		</id>		
		<property column="clientid" name="clientID" type="java.lang.Long"></property>						
		<property column="status" name="status" type="java.lang.Integer"></property>
		<property column="name" name="name" type="java.lang.String"></property>	
		<property column="englishname" name="englishName" type="java.lang.String"></property>																												
		<property column="description" name="description" type="java.lang.String"></property>				
	</class>
</hibernate-mapping>