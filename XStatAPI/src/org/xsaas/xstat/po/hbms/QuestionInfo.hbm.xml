<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="QuestionInfo" table="question_info">
		<id column="questionid" name="questionID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="question" name="question" type="java.lang.String"></property>
		<property column="englishquestion" name="englishQuestion" type="java.lang.String"></property>
		<property column="questiontype" name="questionType" type="java.lang.Integer"></property>
		<property column="zhexplain" name="zhExplain" type="java.lang.String"></property>
		<property column="englishexplain" name="englishExplain" type="java.lang.String"></property>
		<property column="format" name="format" type="java.lang.String"></property>
		<property column="fieldvalue" name="fieldValue" type="java.lang.String"></property>
		<property column="fieldtype" name="fieldType" type="java.lang.Integer"></property>		
		<property column="status" name="status" type="java.lang.Integer"></property>														
	</class>
</hibernate-mapping>