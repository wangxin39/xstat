<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="RemarkInfo" table="remark_info">
		<id column="remarkid" name="remarkID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="clientid" name="clientID" type="java.lang.Long"></property>
		<property column="projectid" name="projectID" type="java.lang.Long"></property>
		<property column="issueid" name="issueID" type="java.lang.Long"></property>
		<property column="employeeid" name="employeeID" type="java.lang.Long"></property>
		<property column="partid" name="partID" type="java.lang.Long"></property>	
		<property column="informantid" name="informantID" type="java.lang.Long"></property>
		<property column="inquisitionid" name="inquisitionID" type="java.lang.Long"></property>
		<property column="strengths" name="strengths" type="java.lang.String"></property>
		<property column="enStrengths" name="enStrengths" type="java.lang.String"></property>	
		<property column="weaknesses" name="weaknesses" type="java.lang.String"></property>
		<property column="enWeaknesses" name="enWeaknesses" type="java.lang.String"></property>	
		<property column="conclusion" name="conclusion" type="java.lang.String"></property>
		<property column="enConclusion" name="enConclusion" type="java.lang.String"></property>		
		<property column="addtime" name="addTime" type="java.sql.Timestamp"></property>		
		<property column="status" name="status" type="java.lang.Integer"></property>	
		<property column="remarkstatus" name="remarkStatus" type="java.lang.Integer"></property>									
	</class>
</hibernate-mapping>