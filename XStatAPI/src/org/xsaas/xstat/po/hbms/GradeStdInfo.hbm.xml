<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="GradeStdInfo" table="grade_std_info">
		<id column="gradeStdID" name="gradeStdID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="inquisitionid" name="inquisitionID" type="java.lang.Long"></property>	
		<property column="title" name="title" type="java.lang.String"></property>
	    <property column="description" name="description" type="java.lang.String"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>	
	</class>
</hibernate-mapping>