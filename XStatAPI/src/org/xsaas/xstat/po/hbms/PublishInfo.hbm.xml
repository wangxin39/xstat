<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="PublishInfo" table="publish_info">
		<id column="publishid" name="publishID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="articleid" name="articleID" type="java.lang.Long"></property>
		<property column="smgkid" name="smgkID" type="java.lang.Long"></property>	
		<property column="type" name="type" type="java.lang.Integer"></property>	
		<property column="status" name="status" type="java.lang.Integer"></property>
		<property column="publishtime" name="publishTime" type="java.sql.Timestamp"></property>									
	</class>
</hibernate-mapping>