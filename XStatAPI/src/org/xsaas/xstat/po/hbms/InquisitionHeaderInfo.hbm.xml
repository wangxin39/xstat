<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="InquisitionHeaderInfo" table="inquisition_header_info">
		<id column="headerid" name="headerID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="inquisitionid" name="inquisitionID" type="java.lang.Long"></property>
		<property column="questionid" name="questionID" type="java.lang.Long"></property>
		<property column="questiontype" name="questionType" type="java.lang.Integer"></property>
		<property column="isinput" name="isinput" type="java.lang.Integer"></property>
		<property column="showOrder" name="showOrder" type="java.lang.Integer"></property>
		<property column="fieldName" name="fieldName" type="java.lang.String"></property>
		<property column="selecttype" name="selectType" type="java.lang.Integer"></property>
		<property column="optionTemplateID" name="optionTemplateID" type="java.lang.Long"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>	
	</class>
</hibernate-mapping>