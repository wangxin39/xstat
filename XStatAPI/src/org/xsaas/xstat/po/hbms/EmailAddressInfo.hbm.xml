<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="EmailAddressInfo" table="email_address_info">
		<id column="addressid" name="addressID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="acid" name="acID" type="java.lang.Long"></property>					
		<property column="name" name="name" type="java.lang.String"></property>				
		<property column="telephone" name="telephone" type="java.lang.String"></property>	
		<property column="address" name="address" type="java.lang.String"></property>				
		<property column="QQ" name="QQ" type="java.lang.String"></property>	
		<property column="MSN" name="MSN" type="java.lang.String"></property>				
		<property column="email" name="email" type="java.lang.String"></property>	
		<property column="mobile" name="mobile" type="java.lang.String"></property>
		<property column="company" name="company" type="java.lang.String"></property>
		<property column="job" name="job" type="java.lang.String"></property>	
		<property column="cityid" name="cityID" type="java.lang.Long"></property>
		<property column="postcode" name="postCode" type="java.lang.String"></property>	
		<property column="notes" name="notes" type="java.lang.String"></property>	
		<property column="status" name="status" type="java.lang.Integer"></property>	
		<property column="province" name="province" type="java.lang.Integer"></property>									
	</class>
</hibernate-mapping>