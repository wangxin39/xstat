<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="ReportTemplateInfo" table="report_template_info">
		<id column="reporttemplateid" name="reportTemplateID" type="java.lang.Long">
			<generator class="identity"/>
		</id>		
		
		<property column="name" name="name" type="java.lang.String"></property>
		<property column="description" name="description" type="java.lang.String"></property>	
		<property column="filename" name="fileName" type="java.lang.String"></property>
		<property column="path" name="path" type="java.lang.String"></property>	
		<property column="addtime" name="addTime" type="java.sql.Timestamp"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>	
		<property column="type" name="type" type="java.lang.Integer"></property>		
	</class>
</hibernate-mapping>