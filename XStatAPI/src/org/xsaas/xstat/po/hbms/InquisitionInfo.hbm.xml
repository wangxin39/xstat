<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="InquisitionInfo" table="inquisition_info">
		<id column="inquisitionid" name="inquisitionID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="clientID" name="clientID" type="java.lang.Long"></property>
		<property column="title" name="title" type="java.lang.String"></property>
		<property column="templateid" name="templateID" type="java.lang.Long"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>
		<property column="inquisitionstatus" name="inquisitionStatus" type="java.lang.Integer"></property>
	</class>
</hibernate-mapping>