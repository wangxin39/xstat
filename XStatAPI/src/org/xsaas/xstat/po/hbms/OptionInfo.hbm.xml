<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="OptionInfo" table="option_info">
		<id column="optionid" name="optionID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="type" name="type" type="java.lang.Integer"></property>
		<property column="chinaoption" name="chinaOption" type="java.lang.String"></property>
		<property column="englishoption" name="englishOption" type="java.lang.String"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>	
	</class>
</hibernate-mapping>