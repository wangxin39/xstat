<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="OutputInfo" table="output_info">
		<id column="outputid" name="outputID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="issueid" name="issueID" type="java.lang.Long"></property>
		<property column="informantid" name="informantID" type="java.lang.Long"></property>
		<property column="clientid" name="clientID" type="java.lang.Long"></property>
		<property column="filetype" name="fileType" type="java.lang.Integer"></property>
		<property column="filename" name="fileName" type="java.lang.String"></property>
		<property column="makeTime" name="makeTime" type="java.sql.Timestamp"></property>
		<property column="savepath" name="savePath" type="java.lang.String"></property>
		<property column="status" name="status" type="java.lang.Integer"></property>												
	</class>
</hibernate-mapping>