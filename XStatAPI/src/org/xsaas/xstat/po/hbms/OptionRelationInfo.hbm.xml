<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.xsaas.xstat.po">
	<class name="OptionRelationInfo" table="option_relation_info">
		<id column="optionrelationid" name="optionRelationID" type="java.lang.Long">
			<generator class="identity"/>
		</id>
		<property column="optionTemplateID" name="optionTemplateID" type="java.lang.Long"></property>	
		<property column="shownum" name="showNum" type="java.lang.String"></property>
		<property column="optionname" name="optionName" type="java.lang.String"></property>
		<property column="alterID" name="alterID" type="java.lang.Long"></property>	
		<property column="optionID" name="optionID" type="java.lang.Long"></property>	
		<property column="status" name="status" type="java.lang.Integer"></property>	
	</class>
</hibernate-mapping>